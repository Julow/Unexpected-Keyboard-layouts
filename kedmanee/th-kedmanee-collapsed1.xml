<keyboard name="Kedmanee (TH)" script="thai">
  <row>    
    <key c ="/" sw ="1" nw ="ๅ" ne="!" se="`"/>
    <key c="-" sw="2" ne="@" nw="loc esc"/>
    <key c="ภ" sw="3" ne="#"/>
    <key c="ถ" sw="4" ne="$"/>
    <key c="ุ" sw="5" ne="%"/>
    <key c="ึ" sw="6" ne="^"/>
    <key  c="ค" sw="7" ne="&amp;"/>
    <key c="ต" sw="8" ne="*"/>
    <key c="จ" sw="9" ne="("/>
    <key c="ข" sw="0" ne=")"/>
    <key c="ช" se="=" ne="+" sw="-" nw="_"/>
  </row>
  <row>
    <key c="ไ" nw="ๆ"  ne="loc tab"/>
    <key c="ำ"/>
    <key c="พ"/>
    <key c="ะ"/>
    <key c="ั"/>
    <key c="ี" se="loc redo"/>
    <key c="ร"/>
    <key c="น"/>
    <key c="ย"/>
    <key c="บ" sw="[" ne="{"/>
    <key c="ล" sw="]" ne="}"/>
  </row>
  <row>
    <key c="ห" nw="ฟ" se="loc selectAll"/>
    <key c="ก"/>
    <key c="ด"/>
    <key c="เ"/>
    <key c="้"/>
    <key c="่"/>
    <key c="า"/>
    <key c="ส"/>
    <key c="ว" ne=":" sw=";"/>
    <key c="ง" ne='"' sw="'"/>
    <key c="ฃ" ne="|" sw="\\"/>
  </row>
  <row>
    <key c="shift" ne="loc capslock"/>
    <key c="ป" nw="ผ" se="loc undo"/>
    <key c="แ" se="loc cut"/>
    <key c="อ" se="loc copy"/>
    <key c="ิ" se="loc paste" nw="loc pasteAsPlainText"/>
    <key c="ื"/>
    <key c="ท"/>
    <key c="ม" ne="&lt;" sw="."/>
    <key c="ใ" ne="&gt;" sw=","/>
    <key c="ฝ" ne="\?" sw="/"/>
    <key c="backspace" ne="delete"/>
  </row>
  <modmap>
  <shift a="_" b="%" />
  <shift a="ๅ" b="+" />
  <shift a="/" b="๑" />
  <shift a="-" b="๒" />
  <shift a="ภ" b="๓" />
  <shift a="ถ" b="๔" />
  <shift a="ุ" b="ู" />
  <shift a="ึ" b="฿" />
  <shift a="ค" b="๕" />
  <shift a="ต" b="๖" />
  <shift a="จ" b="๗" />
  <shift a="ข" b="๘" />
  <shift a="ช" b="๙" /> 

  <shift a="ๆ" b="๐" />
  <shift a="ไ" b='"' />
  <shift a="ำ" b="ฎ" />
  <shift a="พ" b="ฑ" />
  <shift a="ะ" b="ธ" />
  <shift a="ั" b="ํ" />
  <shift a="ี" b="๊" />
  <shift a="ร" b="ณ" />
  <shift a="น" b="ฯ" />
  <shift a="ย" b="ญ" />
  <shift a="บ" b="ฐ" />
  <shift a="ล" b="," />
  <shift a="ฃ" b="ฅ" /> 

  <shift a="ฟ" b="ฤ" />
  <shift a="ห" b="ฆ" />
  <shift a="ก" b="ฏ" />
  <shift a="ด" b="โ" />
  <shift a="เ" b="ฌ" />
  <shift a="้" b="็" />
  <shift a="่" b="๋" />
  <shift a="า" b="ษ" />
  <shift a="ส" b="ศ" />
  <shift a="ว" b="ซ" />
  <shift a="ง" b="." /> 

  <shift a="ผ" b="(" />
  <shift a="ป" b=")" />
  <shift a="แ" b="ฉ" />
  <shift a="อ" b="ฮ" />
  <shift a="ิ" b="ฺ" />
  <shift a="ื" b="์" />
  <shift a="ท" b="?" />
  <shift a="ม" b="ฒ" />
  <shift a="ใ" b="ฬ" />
  <shift a="ฝ" b="ฦ" />

  <shift a="`" b="~" />
  </modmap>
</keyboard> 

  